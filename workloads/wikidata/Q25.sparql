SELECT ?var1 ?var1Label ?var2 ?var3 ?var4 
WHERE {
  ?var1 <http://www.wikidata.org/prop/direct/P106> <http://www.wikidata.org/entity/Q4964182> .
  ?var1 <http://www.wikidata.org/prop/direct/P569> ?var2 .
  ?var1 <http://www.wikidata.org/prop/direct/P570> ?var3 .
  ?var1 <http://www.wikidata.org/prop/direct/P27> ?var4 .
  ?var1 <http://www.w3.org/2000/01/rdf-schema#label> ?var1Label .
  FILTER ( LANG(?var1Label) = "en" ) .
}
ORDER BY DESC( ?var2 )
LIMIT 100
